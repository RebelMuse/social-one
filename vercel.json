{
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/legal/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        },
        {
          "key": "X-Robots-Tag",
          "value": "all"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/legal/privacy",
      "destination": "/legal/privacy.html"
    }
  ]
} 